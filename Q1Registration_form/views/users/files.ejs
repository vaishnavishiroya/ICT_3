<h1>Uploaded Files</h1>

<% users.forEach(user => { %>
    <h3><%= user.name %> (<%= user.email %>)</h3>
    <ul>
        <% user.files.forEach(file => { %>
            <li>
                <%= file %> 
                <a href="/users/download/<%= file %>">Download</a>
            </li>
        <% }) %>
    </ul>
<% }) %>
